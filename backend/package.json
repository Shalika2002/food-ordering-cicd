{
  "name": "food-ordering-backend",
  "version": "1.0.0",
  "description": "Backend API for Food Ordering System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seed.js",
    "test": "jest --testPathIgnorePatterns=tests/ui/",
    "test:watch": "jest --watch --testPathIgnorePatterns=tests/ui/",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=tests/ui/",
    "test:bdd": "cucumber-js",
    "test:ui": "jest --testPathPatterns=tests/ui/ --testTimeout=60000",
    "test:api": "jest --testPathPatterns=tests/api/ --testTimeout=30000",
    "test:unit": "jest --testPathPatterns=tests/unit/ --testTimeout=10000",
    "test:all": "npm run test:unit && npm run test:api && npm run test:bdd",
    "test:all-with-ui": "npm run test:unit && npm run test:api && npm run test:ui && npm run test:bdd"
  },
  "keywords": [
    "food",
    "ordering",
    "express",
    "mongodb",
    "api"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "chromedriver": "^140.0.3",
    "jest": "^30.1.3",
    "jest-environment-node": "^30.1.2",
    "jest-html-reporters": "^3.1.7",
    "mongodb-memory-server": "^10.2.1",
    "nodemon": "^3.0.1",
    "selenium-webdriver": "^4.35.0",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
